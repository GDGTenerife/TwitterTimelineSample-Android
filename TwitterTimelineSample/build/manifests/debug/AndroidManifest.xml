<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.dhsoftware.android.TwitterTimelineSample">

    <uses-sdk android:minSdkVersion="10" android:targetSdkVersion="16"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/com_dhsoftware_android_twitterTimelineSample_app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/com_dhsoftware_android_twitterTimelineSample_app_name" android:name="com.dhsoftware.android.TwitterTimelineSample.activities.TimelineActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
